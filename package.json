{
  "name": "cy-network-viewer",
  "author": "Keiichiro Ono <kono@ucsd.edu>",
  "version": "v2.0.0",
  "description": "React-based graph (network) visualizer component",
  "license": "MIT",
  "keywords": [
    "cytoscape",
    "visualization",
    "graph",
    "bioinformatics"
  ],
  "main": "build/CyNetworkViewer.js",
  "files": [
    "webpack.config.js",
    ".preprocessor.js",
    ".travis.yml",
    "build/*",
    "src/*"
  ],
  "scripts": {
    "build": "webpack --progress --colors",
    "clean": "rm build/*",
    "lint": "eslint src/; exit 0",
    "test": "eslint src/ && jest",
    "coverage": "jest --coverage"
  },
  "homepage": "https://github.com/CyComponent/CyNetworkViewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyComponent/CyNetworkViewer.git"
  },
  "bugs": {
    "url": "https://github.com/CyComponent/CyNetworkViewer/issues"
  },
  "dependencies": {
    "cy-network-store": "^2.0.0",
    "cy-network-viewer-component": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.7.1",
    "file-loader": "^0.8.5",
    "jest": "^16.0.1",
    "node-sass": "^3.4.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-link": "^0.2.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/.preprocessor.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true
    }
  }
}
